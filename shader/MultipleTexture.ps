#version 400

//input
in vec2 outTexCoord;
in vec3 outNormal;

//output
out vec4 resultColor;

//uniform
uniform sampler2D sampler;
uniform sampler2D sampler2;


//FragmentShader
void main(void)
{
	vec4 c1 = texture2D(sampler, outTexCoord);
	vec4 c2 = texture2D(sampler2, outTexCoord);
	resultColor = c1 * c2;
}
